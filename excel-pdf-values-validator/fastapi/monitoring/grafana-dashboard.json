{
  "dashboard": {
    "id": null,
    "title": "Autonomous Agent System Dashboard",
    "tags": ["autonomous-agents", "celery", "memory-management"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Memory Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "system_memory_used_percent",
            "legendFormat": "Memory Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1}
      },
      {
        "id": 3,
        "title": "Available Memory",
        "type": "stat",
        "targets": [
          {
            "expr": "system_memory_available_gb",
            "legendFormat": "Available GB"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "unit": "bytes",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 3}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 1}
      },
      {
        "id": 4,
        "title": "CPU Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "system_cpu_percent",
            "legendFormat": "CPU %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 1}
      },
      {
        "id": 5,
        "title": "Memory Threshold Level",
        "type": "stat",
        "targets": [
          {
            "expr": "memory_threshold_level",
            "legendFormat": "Threshold"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 3,
            "mappings": [
              {"options": {"0": {"text": "CRITICAL"}}, "type": "value"},
              {"options": {"1": {"text": "LOW"}}, "type": "value"},
              {"options": {"2": {"text": "MEDIUM"}}, "type": "value"},
              {"options": {"3": {"text": "HIGH"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 2}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 1}
      },
      {
        "id": 6,
        "title": "Agent Activity",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9}
      },
      {
        "id": 7,
        "title": "Active Agents",
        "type": "timeseries",
        "targets": [
          {
            "expr": "active_agents_count",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10}
      },
      {
        "id": 8,
        "title": "Processing Mode",
        "type": "stat",
        "targets": [
          {
            "expr": "processing_mode",
            "legendFormat": "Mode"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 2,
            "mappings": [
              {"options": {"0": {"text": "MINIMAL"}}, "type": "value"},
              {"options": {"1": {"text": "CONSOLIDATED"}}, "type": "value"},
              {"options": {"2": {"text": "DISTRIBUTED"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 2}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10}
      },
      {
        "id": 9,
        "title": "Task Performance",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 18}
      },
      {
        "id": 10,
        "title": "Task Execution Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(agent_tasks_total[5m])",
            "legendFormat": "{{agent_type}} ({{status}})"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 19}
      },
      {
        "id": 11,
        "title": "Task Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_task_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile {{agent_type}}"
          },
          {
            "expr": "histogram_quantile(0.50, rate(agent_task_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile {{agent_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 19}
      },
      {
        "id": 12,
        "title": "Memory Usage by Agent",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_memory_usage_mb_bucket[5m]))",
            "legendFormat": "95th percentile {{agent_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "unit": "bytes"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 27}
      },
      {
        "id": 13,
        "title": "Task Failures",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(task_failures_total[5m])",
            "legendFormat": "{{task_type}} ({{error_type}})"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 27}
      },
      {
        "id": 14,
        "title": "Processing Quality",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 35}
      },
      {
        "id": 15,
        "title": "PDF Processing Volume",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(pdf_processing_chunks_count_bucket[5m]))",
            "legendFormat": "95th percentile chunks"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 36}
      },
      {
        "id": 16,
        "title": "Excel Processing Volume",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(excel_processing_rows_count_bucket[5m]))",
            "legendFormat": "95th percentile rows"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 36}
      },
      {
        "id": 17,
        "title": "Validation Accuracy",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(validation_accuracy_percent_bucket[5m]))",
            "legendFormat": "95th percentile accuracy"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 36}
      },
      {
        "id": 18,
        "title": "Consolidation Events",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(consolidation_events_total[5m])",
            "legendFormat": "{{trigger_reason}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 44}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "name": "Consolidation Events",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(consolidation_events_total[1m]) > 0",
          "textFormat": "Consolidation triggered"
        }
      ]
    },
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1
  }
}
